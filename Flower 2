import turtle
import turtle as tur
import colorsys as cs
t = turtle.Turtle()
for i in range (180):
    tur.bgcolor('black')
    t.speed(0)
    t.color('white')
    t.circle(190 - i,90)
    t.left(90)
    t.color('#E38B29')
    t.circle(190 - i, 90)
    t.left(18)
    t.color('#E38B60')
    t.circle(90 - i, 60)
    t.left(18)
    t.color('blue')
    t.circle(190 - i,10)
    t.left(19)